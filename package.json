{
  "name": "nuxt-template",
  "version": "0.0.1",
  "description": "Basic Nuxt project implementation",
  "author": "Colin Baxter",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "deploy:heroku": "git push heroku master",
    "heroku-postbuild": "npm run build",
    "styleguide": "vue-styleguidist server --open --config styleguide.config.js",
    "styleguide:build": "vue-styleguidist build --config styleguide.config.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/pwa": "^2.0.5",
    "@zaneray/vue-components": "^0.1.3",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.28",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "gsap": "^1.20.3",
    "js-cookie": "^2.2.1",
    "nuxt": "2.3.4",
    "nuxt-sass-resources-loader": "^2.0.5",
    "prismic-dom": "^2.0.6",
    "prismic-javascript": "^1.5.1",
    "prismic-nuxt": "^0.2.0",
    "prismic-toolbar": "^0.0.8",
    "vue-mq": "^1.0.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "css-loader": "^1.0.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.23.1",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-styleguidist": "^2.1.2",
    "webpack": "^4.39.2",
    "webpack-merge": "^4.2.1"
  }
}
