{
  "name": "nuxt-template",
  "version": "0.0.1",
  "description": "Basic Nuxt project implementation",
  "author": "Colin Baxter",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "deploy:heroku": "git push heroku master",
    "heroku-postbuild": "npm run build",
    "styleguide": "vue-styleguidist server --config styleguide.config.js",
    "styleguide:build": "vue-styleguidist build --config styleguide.config.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.0.5",
    "axios": "^0.17.1",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-beta",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "gsap": "^1.20.3",
    "js-cookie": "^2.2.0",
    "nuxt": "^2.3.4",
    "nuxt-sass-resources-loader": "^2.0.5",
    "prismic-dom": "^2.0.6",
    "prismic-javascript": "^1.5.0-beta.1",
    "prismic-nuxt": "^0.2.0",
    "prismic-toolbar": "^0.0.8",
    "vue-mq": "^1.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.7",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.23.1",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-styleguidist": "^2.0.2",
    "webpack": "^4.28.0",
    "webpack-merge": "^4.1.2"
  }
}
